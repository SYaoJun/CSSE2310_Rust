        "Check uqentropy with --leet argument handles single password file with only single substitutions of letters with two substitute symbols" \
        -timeout 5 \
        -maxattempts 1 \
        -stdin "testfiles/11.3.in" \
        -exit 0 \
        -stdout testfiles/11.3.stdout \
        -stderr testfiles/empty \
        "./uqentropy" '--leet' 'testfiles/11.3.file'
}

