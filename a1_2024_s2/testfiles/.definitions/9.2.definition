# Details for test 9.2. ($1 will be run_test, explain_test or mark_test)
function test9.2() {
    "$1" 9.2 "digit_check02" \
        "Check uqentropy with --digit-append argument handles single password file with single all-letter password with 1 appended digit and no matches in candidate passwords" \
        -timeout 5 \
        -maxattempts 1 \
        -stdin "testfiles/9.2.in" \
        -exit 14 \
        -stdout testfiles/9.2.stdout \
        -stderr testfiles/empty \
        "./uqentropy" '--digit-append' '1' 'testfiles/9.2.file'
}

