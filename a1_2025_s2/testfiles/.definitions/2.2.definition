        "Check uqbasejump reports a file error with argument(s) '--inputfile /etc/shadow'" \
        --timeout 3 \
        --maxattempts 1 \
        --stdin "/dev/null" \
        --exit 16 \
        --stdout testfiles/empty \
        --stderr testfiles/2.2.stderr \
        "./uqbasejump" '--inputfile' '/etc/shadow'
}
