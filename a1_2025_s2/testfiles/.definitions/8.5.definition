# Details for test 8.5.
# $1 will be run_test, explain_test or mark_test.
function test8.5() {
    "$1" 8.5 "evaluate_single_stdin05" \
        "Check uqbasejump operates correctly when the newline character is entered via stdin to evaluate an expression consisting of a single number with two invalid characters at the end" \
        --timeout 5 \
        --maxattempts 1 \
        --stdin "testfiles/8.5.in" \
        --exit 0 \
        --stdout testfiles/8.5.stdout \
        --stderr testfiles/empty \
        "./uqbasejump" '--obases' '2,10,16' '--inbase' '2'
}
