# Details for test 10.3.
# $1 will be run_test, explain_test or mark_test.
function test10.3() {
    "$1" 10.3 "multiple_options03" \
        "Check uqexpr operates correctly when given multiple command line arguments and commands with a single loop variable on the command line and a variety of simple expressions (no assignments) on input" \
        --timeout 5 \
        --maxattempts 1 \
        --stdin "testfiles/10.3.in" \
        --exit 0 \
        --stdout testfiles/10.3.stdout \
        --stderr testfiles/10.3.stderr \
        "./uqexpr" '--forloop' 'x,1,1,22'
}
