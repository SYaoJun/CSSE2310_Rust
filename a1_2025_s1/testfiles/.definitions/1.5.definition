        "Check uqexpr reports a usage error with argument(s) '--significantfigures 0'" \
        --timeout 3 \
        --maxattempts 1 \
        --stdin "/dev/null" \
        --exit 18 \
        --stdout testfiles/empty \
        --stderr testfiles/1.5.stderr \
        "./uqexpr" '--significantfigures' '0'
}
