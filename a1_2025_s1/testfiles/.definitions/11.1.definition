# Details for test 11.1.
# $1 will be run_test, explain_test or mark_test.
function test11.1() {
    "$1" 11.1 "loop_variables01" \
        "Check uqexpr operates correctly using the @range and @loop commands with a single loop variable defined on the command line and a single @loop command from stdin - evaluating an expression not including the variable" \
        --timeout 5 \
        --maxattempts 1 \
        --stdin "testfiles/11.1.in" \
        --exit 0 \
        --stdout testfiles/11.1.stdout \
        --stderr testfiles/empty \
        "./uqexpr" '--forloop' 'x,1,1,10'
}
