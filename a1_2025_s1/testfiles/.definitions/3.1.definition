# Details for test 3.1.
# $1 will be run_test, explain_test or mark_test.
function test3.1() {
    "$1" 3.1 "invalid_variables01" \
        "Check uqexpr reports invalid or duplicate variable errors with argument(s) '--init a'" \
        --timeout 3 \
        --maxattempts 1 \
        --stdin "/dev/null" \
        --exit 14 \
        --stdout testfiles/empty \
        --stderr testfiles/3.1.stderr \
        "./uqexpr" '--init' 'a'
}
