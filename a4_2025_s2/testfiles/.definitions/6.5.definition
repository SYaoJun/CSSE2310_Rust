# Details for test 6.5.
# $1 will be run_test, explain_test or mark_test.
function test6.5() {
    "$1" 6.5 "alphanumeric_stdin05" \
        "Check uqbasejump operates correctly when alphanumeric characters are entered via stdin with two invalid characters at the end" \
        --timeout 5 \
        --maxattempts 1 \
        --stdin "testfiles/6.5.in" \
        --exit 0 \
        --stdout testfiles/6.5.stdout \
        --stderr testfiles/empty \
        "./uqbasejump" '--obases' '2,10,16' '--inbase' '2'
}
