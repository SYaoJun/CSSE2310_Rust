# Details for test 2.3.
# $1 will be run_test, explain_test or mark_test.
function test2.3() {
    "$1" 2.3 "client_connection03" \
        "Check ratsclient handles inability to connect to port specified as a service name" \
        --timeout 15 \
        --maxattempts 5 \
        --stdin "/dev/null" \
        --exit 8 \
        --stdout testfiles/empty \
        --stderr testfiles/2.3.stderr \
        "./ratsclient" 'player' 'game' 'at-rtmp'
}
