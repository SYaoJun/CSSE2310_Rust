# Details for test 10.1.
# $1 will be run_test, explain_test or mark_test.
function test10.1() {
    "$1" 10.1 "server_single_game01" \
        "Check ratsserver handles a single client connecting" \
        --timeout 15 \
        --maxattempts 5 \
        --stdin "/dev/null" \
        --exit 0 \
        --stdout testfiles/10.1.stdout \
        --stderr testfiles/empty \
        --file "/tmp/csse2310.totalthreadcount.txt" "testfiles/10.1.csse2310.totalthreadcount.txt" \
        "testfiles/check_server_sequence.sh" '0' 'hi' 'testfiles/10.1.sequence'
}
