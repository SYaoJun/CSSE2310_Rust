# Details for test 14.2.
# $1 will be run_test, explain_test or mark_test.
function test14.2() {
    "$1" 14.2 "multiple_options_stdin02" \
        "Check uqbasejump operates correctly when given multiple command line arguments, arithmetic expressions, key presses, and commands entered via stdin - several command line arguments, arithmetic expressions, key presses and commands" \
        --timeout 5 \
        --maxattempts 1 \
        --stdin "testfiles/14.2.in" \
        --exit 0 \
        --stdout testfiles/14.2.stdout \
        --stderr testfiles/empty \
        "./uqbasejump" '--inbase' '10' '--obases' '16,2,10'
}
