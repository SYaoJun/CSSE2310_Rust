        "Check uqentropy with --leet argument handles single password file with only single substitutions of letters with only one substitute symbol" \
        -timeout 5 \
        -maxattempts 1 \
        -stdin "testfiles/11.2.in" \
        -exit 14 \
        -stdout testfiles/11.2.stdout \
        -stderr testfiles/empty \
        "./uqentropy" '--leet' 'testfiles/11.2.file'
}

